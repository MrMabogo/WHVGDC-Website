      <script type="text/javascript" src="https://cdn.rawgit.com/LeaVerou/awesomplete/gh-pages/awesomplete.min.js"> </script>
      <script type="text/javascript">
         var input = document.getElementById("txtSearch");
         var awesomplete = new Awesomplete(input, {
              list: ["gamepage", "team members", "main page", "somethingsomething page"],
              minChars: 1
          });
          document.getElementById('Search').onsubmit = function() {
              window.location = 'http://www.google.com/search?q= (our domain)' + document.getElementById('txtSearch').value;
              return false;
          }
      </script>
